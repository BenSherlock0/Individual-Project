<Window x:Class="Individual_Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Individual_Project"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <ListBox x:Name="lbxEntries" HorizontalAlignment="Left" Height="270" Margin="62,68,0,0" VerticalAlignment="Top" Width="157"/>
        <TextBox x:Name="tbxFirst" HorizontalAlignment="Left" Height="23" Margin="366,68,0,0" TextWrapping="Wrap" Text="Paul" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbxLast" HorizontalAlignment="Left" Height="24" Margin="366,111,0,0" TextWrapping="Wrap" Text="Sherlock" VerticalAlignment="Top" Width="120"/>
        <TextBlock HorizontalAlignment="Left" Margin="688,323,0,0" TextWrapping="Wrap" Text="66" VerticalAlignment="Top" Height="76" Width="80" FontSize="36" TextAlignment="Center"/>
        <Button x:Name="btnSubmit" Content="Submit" HorizontalAlignment="Left" Margin="366,277,0,0" VerticalAlignment="Top" Width="74" Click="btnSubmit_Click"/>
        <Label Content="Entries" HorizontalAlignment="Left" Margin="62,25,0,0" VerticalAlignment="Top"/>
        <Label Content="First Name:" HorizontalAlignment="Left" Margin="257,68,0,0" VerticalAlignment="Top" Height="23" Width="67"/>
        <Label Content="Last Name:" HorizontalAlignment="Left" Margin="257,111,0,0" VerticalAlignment="Top" Height="24" Width="67"/>
        <Label Content="Number 1:" HorizontalAlignment="Left" Margin="257,158,0,0" VerticalAlignment="Top" Width="67" Height="23"/>
        <Label Content="Number 2:" HorizontalAlignment="Left" Margin="257,193,0,0" VerticalAlignment="Top" Width="67" Height="23"/>
        <Label Content="Number 3:" HorizontalAlignment="Left" Margin="257,232,0,0" VerticalAlignment="Top" Width="67" Height="23" RenderTransformOrigin="0.571,-0.435"/>
        <TextBox x:Name="tbxNum1" HorizontalAlignment="Left" Height="23" Margin="366,158,0,0" TextWrapping="Wrap" Text="5" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbxNum2" HorizontalAlignment="Left" Height="23" Margin="366,193,0,0" TextWrapping="Wrap" Text="13" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbxNum3" HorizontalAlignment="Left" Height="23" Margin="366,232,0,0" TextWrapping="Wrap" Text="33" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
